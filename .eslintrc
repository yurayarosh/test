{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "extends": ["airbnb-base", "plugin:prettier/recommended"],
  "rules": {
    "no-plusplus": ["error", { "allowForLoopAfterthoughts": true }],
    "no-unused-vars": 1,
    "no-underscore-dangle": 0,
    "no-param-reassign": 0
  }
}
